package day16;
//import java.lang.String.*;

public class EvenOdd {
	public static void main(String[] args) {
		
			int num = 0;
	        String odd = ""; 
	        String even = "";​
	        while (num <= 50) {​
	            if (num % 2 == 0) {
	                even += num + " ";
	            } else {
	                odd += num + " ";
	            }​
	            ++num;
	        }
	        System.out.println(even);
	        System.out.println(odd);
		
	}

}
